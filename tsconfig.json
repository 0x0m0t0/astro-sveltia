{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"moduleResolution": "bundler",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@layouts/*": ["src/layouts/*"],
			"@components/*": ["src/components/*"],
			"@content/*": ["src/content/*"],
			"@site/*": ["src/site/*"],
			"@styles/*": ["src/styles/*"],
			"@utils/*": ["src/utils/*"]
		}
	}
}
